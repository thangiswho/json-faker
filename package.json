{
  "name": "simple-faker",
  "version": "1.0.0",
  "description": "simple-faker generates massive amount of json fake data with zero coding",
  "main": "./dist/index.js",
  "bin": {"fake-data": "./bin/fake-data.js"},
  "types": "./dist/index.d.ts",
  "scripts": {
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "build:lint": "tslint -p tsconfig.json",
    "build:format": "prettier --write \"**/*.ts\" \"**/*.js\"",
    "build:compile": "tsc",
    "build": "npm-run-all --serial clean build:*",
    "test": "jest",
    "prepare": "yarn run build",
    "preversion": "yarn run test",
    "version": "yarn run build:format && git add -A src",
    "postversion": "git push --tags && yarn publish . --tag $npm_package_version && git push && echo \" version $npm_package_version was successfully released!\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thangiswho/simple-faker.git"
  },
  "files": [
    "dist",
    "bin"
  ],
  "keywords": [
    "json",
    "faker",
    "api"
  ],
  "author": "thangiswho <thangiswho@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thangiswho/simple-faker/issues"
  },
  "homepage": "https://github.com/thangiswho/simple-faker#readme",
  "devDependencies": {
    "@types/faker": "^5.5.0",
    "@types/jest": "^26.0.22",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "faker": "^5.5.2",
    "yargs": "^16.2.0"
  }
}
